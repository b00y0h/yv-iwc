{
  "name": "@ux_bob/yv-iwc",
  "version": "2.1.2",
  "private": false,
  "description": "A React component for displaying YouVisit IWCs with syntax highlighting.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "sideEffects": [
    "*.css"
  ],
  "packageManager": "pnpm@9.3.0",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vitest",
    "dev:build": "tsup src/index.ts --format esm,cjs --dts --external react,react-dom --sourcemap --clean --out-dir dist --watch",
    "dev:example": "pnpm --filter testing dev",
    "dev:all": "concurrently \"pnpm run dev:build\" \"pnpm run dev:example\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "tsup src/index.ts --format esm,cjs --dts --external react,react-dom --sourcemap --clean --out-dir dist",
    "build:all": "pnpm run build && pnpm run build --filter testing",
    "lint": "tsc --noEmit",
    "lint:fix": "tsc --noEmit",
    "clean": "rm -rf dist && rm -rf node_modules/.cache",
    "ci": "pnpm run lint && pnpm run test && pnpm run build",
    "prerelease": "pnpm run ci",
    "release": "changeset publish",
    "version": "changeset version",
    "changeset": "changeset",
    "changeset:version": "changeset version && pnpm install --lockfile-only",
    "changeset:publish": "pnpm run build && changeset publish",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.2",
    "@eslint/js": "^9.22.0",
    "@types/react": "^19.0.10",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.2.0",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "tsup": "^8.4.0",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.3.3",
    "vitest": "^3.1.2"
  }
}
